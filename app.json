{
  "expo": {
    "name": "Euro 24",
    "slug": "euro",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/euroicon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo_big.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/euroicon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.arekmarko8.euro"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "3a9045f3-a33a-4303-8174-05b0c52df8bd"
      }
    }
  }
}
